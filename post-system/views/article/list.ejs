<!DOCTYPE html>
<html>
	<head>
		<title><%= title %></title>
	</head>
	<body>
		<a href="/article/post">Post</a>
		<ul>
			<% articles.forEach(function(article) { %>
				<li>
					<a href="/article/<%=article._id %>/<%=article.author._id %>">
						<%=article.title %>
						(<%=article.author.name %>)
					</a>
				</li>
			<% }); %>
		</ul>
		<a href="/">Go Home</a>
	</body>
</html>